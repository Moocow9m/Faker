{
  "name": "moocow9m/faker",
  "type": "library",
  "description": "Faker is a PHP library that generates fake data for you.",
  "keywords": [
    "faker",
    "fixtures",
    "data"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Fran√ßois Zaninotto",
      "role": "Original author"
    },
    {
      "name": "Moocow9m",
      "role": "Continuing development for internl use"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "ext-intl": "*",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.15.0",
    "squizlabs/php_codesniffer": "^3.7.1"
  },
  "autoload": {
    "psr-4": {
      "Faker\\": "src/Faker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Faker\\Test\\": "test/Faker/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "lint": "phpcs --extensions=php --standard=PSR2 src",
    "lint-errors": "phpcs --extensions=php --standard=PSR2 -n src",
    "lint-fix": "phpcbf --extensions=php --standard=PSR2 src test"
  }
}
